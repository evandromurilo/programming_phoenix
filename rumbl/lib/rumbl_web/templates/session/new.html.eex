<h1>Login</h1>

<%= form_for @changeset, Routes.session_path(@conn, :create), fn f -> %>
  <div>
    <%= text_input f, :username, placeholder: "Username" %>
    <%= error_tag f, :username %>
  </div>

  <div>
    <%= password_input f, :password, placeholder: "Password" %>
    <%= error_tag f, :password %>
  </div>

  <%= submit "Login" %>
<% end %>
